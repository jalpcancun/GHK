<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="assets_common" inherit_id="web.assets_common" name="Web Editor Common Assets (used in backend interface and website)">
    <xpath expr="script[last()]" position="after">
        <script type="text/javascript" src="/web_elearning_video/static/src/js/summernote.js"/>
    </xpath>
</template>
<template id="assets_frontend_video" inherit_id="portal.assets_frontend" name="Website Assets">
    <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/web_elearning_video/static/src/js/snippets_animation.js"/>
    </xpath>
</template>
<template id="assets_wysiwyg_video" inherit_id="web_editor.assets_wysiwyg">
    <xpath expr="script[last()]" position="after">
        <script type="text/javascript" src="/web_elearning_video/static/src/js/video_dialog.js"/>
        <script type="text/javascript" src="/web_elearning_video/static/src/js/audio_dialog.js"/>
        <script type="text/javascript" src="/web_elearning_video/static/src/js/wysiwyg.js"/>
    </xpath>
</template>
</odoo>
